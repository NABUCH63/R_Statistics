library(dplyr)
library(tidyverse)
mpg_data <- read.csv("Data/MechaCar_mpg.csv")
print(mpg_data)
regression <- lm(mpg ~ vehicle_length + vehicle_weight + spoiler_angle + ground_clearance + AWD, mpg_data)
print(regression)
pairs(~mpg + vehicle_length + ground_clearance, data=mpg_data)
summary(regression)
coil_data <- read.csv("Data/Suspension_Coil.csv")
total_summary <- summarise(coil_data, MEAN = mean(coil_data$PSI), MEDIAN = median(coil_data$PSI), VARIANCE = var(coil_data$PSI), STDEV = sd(coil_data$PSI))
grouping_coil <- group_by(coil_data, Manufacturing_Lot)
lot_summary <- summarise(grouping_coil, MEAN = mean(PSI), MEDIAN = median(PSI), VARIANCE = var(PSI), STDEV = sd(PSI))
print(total_summary, digits = 8)
print(lot_summary, digits = 8)
mu <- 1500
All_Lots <- t.test(coil_data$PSI, mu=mu)
Lot1_data <- subset(coil_data, Manufacturing_Lot == "Lot1")
Lot2_data <- subset(coil_data, Manufacturing_Lot == "Lot2")
Lot3_data <- subset(coil_data, Manufacturing_Lot == "Lot3")
Lot1 <- t.test(Lot1_data$PSI, mu=mu)
Lot2 <- t.test(Lot2_data$PSI, mu=mu)
Lot3 <- t.test(Lot3_data$PSI, mu=mu)
print(All_Lots)
print(Lot1)
print(Lot2)
print(Lot3)
